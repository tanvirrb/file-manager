{
  "name": "file-manager",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "test": "NODE_ENV=test mocha \"./test/**/*.test.js\" \"./src/**/*.spec.js\" --timeout 15000 --exit"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "joi": "^17.7.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.6",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "faker": "^5.5.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4"
  }
}
